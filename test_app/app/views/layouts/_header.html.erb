<h1>To-Do</h1>
<div class="links">
	<% if user_signed_in? %>
		<%= current_user.username %>
		<%= link_to "Sign out", destroy_user_session_path, method: :delete %>
	<% else %>
		<%= link_to "Sign in", new_user_session_path %>
			<%= link_to "Create Account", new_user_registration_path %>
	<% end %>
</div>